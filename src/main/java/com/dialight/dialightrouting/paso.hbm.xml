<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.dialight.dialightrouting.Classes" table="PASO">
    <id column="ID_paso" name="id">
      <generator class="increment"/>
    </id>
    <property name="nombre" unique="true" not-null="true" >
      <column name="NOMBRE_PASO"/>
    </property>
    <property name="tiempo" not-null="true">
      <column name="TIEMPO_PASO"/>
    </property>
  
   </class>
</hibernate-mapping>